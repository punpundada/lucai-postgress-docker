{
  "name": "lucai-postgress-docker",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "generate": "drizzle-kit generate",
    "studio": "drizzle-kit studio --port 8778",
    "migrate": "drizzle-kit migrate",
    "build":"bun build ./src/index.ts --outdir ./build --target bun",
    "start":"bun run build/index.js",
    "build:start":"bun run build && bun run start"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.21.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@node-rs/argon2": "^1.8.3",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "lucia": "^3.2.0",
    "nodemailer": "^6.9.13",
    "oslo": "^1.2.0",
    "pg": "^8.11.5",
    "zod": "^3.23.8"
  }
}